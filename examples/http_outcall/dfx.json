{
  "canisters": {
    "http_outcall_backend": {
      "candid": "http_outcall.did",
      "package": "http_outcall_backend",
      "build": "build.sh",
      "main": "src/http_outcall_backend/main.nim",
      "wasm": "main.wasm",
      "type": "custom",
      "metadata": [
        {
          "name": "candid:service"
        }
      ]
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    },
    "canister_http": {
      "enabled": true,
      "log_level": "debug"
    },
    "replica": {
      "subnet_type": "system"
    }
  },
  "output_env_file": ".env",
  "version": 1,
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral",
      "canister_http": {
        "enabled": true
      }
    }
  }
}